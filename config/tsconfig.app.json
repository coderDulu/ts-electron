{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "../env.d.ts",
    "../src/renderer**/*",
    "../src/renderer/**/*.vue",
    "../node_modules/ant-design-vue/typings/global.d.ts",
    "../src/preload/*.d.ts"
  ],
  "exclude": [
    "../src/renderer/**/__tests__/*"
  ],
  "compilerOptions": {
    "rootDir": "../src",
    "composite": true,
    "baseUrl": "../",
    "paths": {
      "@/*": [
        "src/renderer/*"
      ]
    },
    "resolve": {
      "extensions": [".js", ".ts", ".tsx", ".vue"]
    },
    "ignoreDeprecations": "5.0"
  }
}